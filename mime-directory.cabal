name:           mime-directory
version:        0.6.0
author:         Mathieu Boespflug <mboes - at - tweag.net>
maintainer:     ARJANEN Lo√Øc Jean David <arjanen.loic@gmail.com>
homepage:       http://github.com/Bebere/mime-directory
synopsis:       A library for parsing/printing the text/directory mime type.
description:    A library for parsing\/printing the text\/directory mime type.
category:       Text
license:        LGPL
license-file:   LICENSE
copyright:      (c) 2008. Mathieu Boespflug <mboes - at - tweag.net>
cabal-version:  >= 1.6.0
build-type:     Simple
tested-with:    GHC == 8.0.2

library
  build-depends:
    base >= 3 && < 5,
    base64-string,
    bytestring >= 0.9,
    containers,
    regex-pcre >= 0.94,
    time >= 1.5,
    network-uri >= 2.6
  exposed-modules:
    Codec.MIME.ContentType.Text.Directory
    Data.ByteString.Lazy.Char8.Caseless

source-repository head
  type:     git
  location: git://github.com/Bebere/mime-directory
